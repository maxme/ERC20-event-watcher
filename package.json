{
  "name": "eth-event-watcher",
  "version": "0.1.0",
  "description": "Web tool to watch ethereum events",
  "author": {
    "name": "Maxime Biais",
    "email": "maxime.biais@gmail.com"
  },
  "main": "dist/cli.js",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "coveralls": "^2.13.1",
    "del-cli": "^1.1.0",
    "documentation": "4.0.0-rc.1",
    "eslint": "^4.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^19.0.2",
    "jest-cli": "^20.0.0",
    "watchify": "^3.11.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.3",
    "bootstrap": "^4.1.3",
    "commander": "^2.19.0",
    "jquery": "^3.3.1",
    "web3": "^1.0.0-beta.37"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "start": "babel-node -- src/cli.js",
    "build": "browserify src/browser.js -o dist/browser.js",
    "build-cli": "babel src -d dist",
    "test": "jest",
    "lint": "eslint src test",
    "lint-fix": "yarn lint -- --fix",
    "clean": "del dist",
    "coverage": "yarn test -- --coverage",
    "watch-js": "watchify src/browser.js -o dist/browser.js",
    "preversion": "yarn lint && yarn test && yarn build && yarn build-cli",
    "prepublish": "yarn lint && yarn test && yarn build && yarn build-cli"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "jest": true,
      "node": true
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT"
}
